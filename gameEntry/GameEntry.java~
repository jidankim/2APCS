public class GameEntry {

    private String _initials;
    private int _score;

    public GameEntry() {
	_initials = "";
	_score = 0;
    }

    public GameEntry(String initials, int socre) {
	_initials = initials;
	_score = score;
    }

    // accessor methods 
    public String getInitials() {
	return _initials;
    }

    public int getScore() {
	return _score;
    }

    public String toString() {
	return _initials + " : " + _score;
    }

}
